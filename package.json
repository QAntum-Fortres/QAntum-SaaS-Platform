{
  "name": "@qantum/prime",
  "version": "28.4.0",
  "description": "QAntum Prime - The Complete Enterprise Testing Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Dimitar Prodromov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/papica777-eng/QA-Framework.git"
  },
  "keywords": [
    "testing",
    "qa",
    "automation",
    "enterprise",
    "ai",
    "performance",
    "security",
    "accessibility",
    "visual-regression"
  ],
  "scripts": {
    "build": "tsc",
    "test": "ts-node tests/index.ts",
    "lint": "eslint src/**/*.ts",
    "docs": "typedoc --out docs/api src/index.ts",
    "system:meditate": "ts-node scripts/system-meditate.ts",
    "synthesis:check": "ts-node -e \"import { Synthesis } from './src/synthesis'; Synthesis.quickCheck().then(console.log)\"",
    "synthesis:full": "ts-node -e \"import { Synthesis } from './src/synthesis'; Synthesis.meditate().then(() => process.exit(0))\"",
    "qantum": "node tools/qantum-cli.js",
    "qantum:watch": "node tools/qantum-cli.js --watch",
    "qantum:status": "node tools/qantum-cli.js --status",
    "qantum:verify": "node tools/qantum-cli.js --verify",
    "qantum:assimilate": "node tools/qantum-cli.js --assimilate",
    "bridge:start": "npx tsx src/reality/kernel/VSCodeBridge.ts",
    "feedback:start": "npx tsx src/reality/kernel/AutonomousFeedbackLoop.ts",
    "sovereign": "npx tsx src/reality/kernel/AutonomousFeedbackLoop.ts --auto-fix",
    "production:launch": "node scripts/sales-autopilot/autonomous-general.js",
    "empire:sync": "node scripts/empire-sync.js",
    "empire:audit": "npx ts-node src/intelligence/SovereignAudit.ts",
    "empire:status": "node tools/qantum-cli.js empire status",
    "loc:count": "node scripts/count-loc.js",
    "guardian": "node scripts/unified-guardian.js",
    "guardian:watch": "node scripts/unified-guardian.js --watch",
    "guardian:eternal": "node scripts/eternal-guardian.js",
    "guardian:heal": "node scripts/eternal-guardian.js --heal",
    "magnet": "npx ts-node src/omega/SovereignMagnet.ts",
    "nucleus": "node scripts/nucleus-mapper.js",
    "ecosystem:scan": "node scripts/unified-guardian.js",
    "ecosystem:watch": "node scripts/unified-guardian.js --watch"
  },
  "bin": {
    "qantum": "./tools/qantum-cli.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@pinecone-database/pinecone": "^6.1.3",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "got-scraping": "^4.1.3",
    "hpagent": "^1.2.0",
    "pdfkit": "^0.17.2",
    "playwright": "^1.57.0",
    "socket.io": "^4.8.3",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "md-to-pdf": "^5.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
